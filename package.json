    "@types/node": "^20.14.14",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "homebridge": "^1.8.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
