{
  "private": false,
  "displayName": "SleepMe Homebridge",
  "name": "@lukewinkates/homebridge-sleepme",
  "version": "0.1.1-alpha.0",
  "description": "HomeKit support for Sleepme Dock Pro devices via Homebridge",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LukeWinikates/homebridge-sleepme.git"
  },
  "bugs": {
    "url": "https://github.com/LukeWinikates/homebridge-sleepme/issues"
  },
  "engines": {
    "node": "^18.17.0 || ^20.9.0",
    "homebridge": "^1.8.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**/*.ts --max-warnings=0",
